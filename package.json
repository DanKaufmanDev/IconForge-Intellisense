{
  "name": "iconforge-intellisense",
  "displayName": "IconForge-Intellisense",
  "publisher": "dkd",
  "icon": "data/IconForge.png",

  "description": "Intellisense for IconForge utility classes.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Visualization",
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DanKaufmanDev/IconForge-Intellisense/"
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:jsx",
    "onLanguage:tsx"
  ],
  "main": "./out/extension.js",
  "files": [
    "out",
    "data"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "vscode-test": "^1.6.1"
  },
  "author": {
    "name": "DanKaufmanDev",
    "url": "https://danielkaufman.dev" 
  }
}
